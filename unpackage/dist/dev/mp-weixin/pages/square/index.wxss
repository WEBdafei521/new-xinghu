.l-title-top {
  position: fixed;
  top: 0rpx;
  width: 100vw;
  z-index: 999;
  background: #FFFFFF;
}
.l-zanwei {
  height: 186rpx;
}
.l-tabs-title {
  width: 100vw;
  padding: 0 100rpx;
  background: #FFFFFF;
}
.l-main-list {
  padding: 20rpx 30rpx;
}
.l-main-list .l-all-list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.l-main-list .l-all-list .l-all-item {
  width: 45vw;
  border-radius: 20rpx;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.l-main-list .l-all-list .l-all-item .l-top {
  position: relative;
  height: 496rpx;
}
.l-main-list .l-all-list .l-all-item .l-top .images {
  width: 100%;
  height: 496rpx;
}
.l-main-list .l-all-list .l-all-item .l-top .l-top-history {
  position: absolute;
  top: 25rpx;
  left: 20rpx;
}
.l-main-list .l-all-list .l-all-item .l-top .l-top-history .l-sum {
  height: 38rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  background: rgba(0, 0, 0, 0.3);
  font-size: 20rpx;
  color: #FFFFFF;
  border-radius: 29rpx;
}
.l-main-list .l-all-list .l-all-item .l-top .l-top-history .l-sum .l-btn {
  height: 100%;
  padding: 5rpx 15rpx;
  background-color: #d6b975;
  border-radius: 29rpx;
}
.l-main-list .l-all-list .l-all-item .l-top .l-top-history .l-sum .l-people {
  padding: 5rpx 15rpx;
}
.l-main-list .l-all-list .l-all-item .l-top .l-bottom-nickname {
  position: absolute;
  bottom: 14rpx;
  left: 20rpx;
}
.l-main-list .l-all-list .l-all-item .l-top .l-bottom-nickname .l-name {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.l-main-list .l-all-list .l-all-item .l-top .l-bottom-nickname .l-name .l-bottom-image {
  width: 46rpx;
  height: 46rpx;
  border-radius: 46rpx;
  overflow: hidden;
  margin-right: 10rpx;
}
.l-main-list .l-all-list .l-all-item .l-top .l-bottom-nickname .l-name .l-bottom-image .images {
  width: 46rpx;
  height: 46rpx;
}
.l-main-list .l-all-list .l-all-item .l-top .l-bottom-nickname .l-name .l-bottom-name {
  font-size: 22rpx;
  font-family: PingFang SC;
  color: white;
}
.l-main-list .l-all-list .l-all-item .l-bottom-tittle {
  padding: 19rpx 0 30rpx 20rpx;
  background: #FFFFFF;
}
.l-main-list .l-all-list .l-all-item .l-bottom-tittle .l-name {
  font-size: 26rpx;
  font-family: PingFang SC;
  color: #333333;
}
.scroll-container {
  padding-top: 76rpx;
  width: 100%;
  height: 100%;
}
.scroll-container .scroll-content {
  width: 100%;
  height: 100%;
}
.scroll-container .scroll-content .scroll-swiper {
  width: 100%;
  height: 100%;
}
.scroll-container .scroll-content .scroll-swiper .scroll-views {
  width: 100%;
  height: 100%;
}

